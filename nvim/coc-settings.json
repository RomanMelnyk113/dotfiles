{
  "eslint.autoFixOnSave": true,
  "prettier.disableSuccessMessage": true,
  "list.source.files.command": "fd",
  "list.source.files.args": [
    "--type",
    "f",
    "--hidden",
    "--follow",
    "--ignore-case",
    "--exclude",
    "'.git'"
  ],
  "list.source.grep.command": "rg",
  "list.source.grep.defaultOptions": ["--interactive", "--auto-preview"],
  "list.source.gstatus.defaultOptions": ["--ignore-case", "--normal"],
  "list.source.buffers.defaultOptions": ["--ignore-case"],
  "list.source.lines.defaultOptions": ["--interactive", "--ignore-case"],
  "coc.source.emoji.filetypes": ["markdown", "gitcommit"],
  "pyright.organizeimports.provider": "isort",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "black",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "python",
    "json",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "dockerfile",
    "go",
    "sh",
    "yaml",
    "yml",
    "html"
  ],
  "coc.preferences.rootPatterns": [
    ".vim",
    ".git",
    ".projections.json",
    "pubspec.lock",
    "pubspec.lock",
    "Gopkg.lock",
    "package.json",
    "Makefile",
    "requirements.txt",
    "venv",
    "Pipfile",
    "Pipfile.lock"
  ],
  "go.goplsArgs": ["-remote=auto", "-logfile", "/tmp/gopls.log"],
  "go.goplsPath": "/Users/roman/.go/bin/gopls",
  "go.formatFlags": ["-s"],
  "yaml.schemas": {
      "kubernetes": "/*.yaml"
  },
  "eslint.packageManager": "yarn",
  "eslint.nodePath": ".yarn/sdks",
  "workspace.workspaceFolderCheckCwd": false
}
